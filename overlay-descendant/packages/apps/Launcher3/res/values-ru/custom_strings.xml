<resources>

    <!-- Settings title to show Google Now at -1 screen on launcher. [CHAR LIMIT=50] -->
    <string name="title_show_google_app">Проведите пальцем, чтобы получить доступ к приложению Google</string>
    <!-- Settings message explaining when the -1 screen is available on an LTR device. [CHAR LIMIT=100] -->
    <string name="msg_minus_one_on_left">На главном экране проведите пальцем влево, чтобы открыть приложение Google</string>
    <!-- Settings message explaining when the -1 screen is available on an RTL device. [CHAR LIMIT=100] -->
    <string name="msg_minus_one_on_right">На главном экране проведите пальцем вправо, чтобы открыть приложение Google</string>
    <string name="pref_show_google_now_summary" translatable="false">@string/msg_minus_one_on_left</string>

    <!-- Suggestions -->
    <string name="suggestion_pref_screen_title">Предложения</string>
    <string name="suggestion_pref_screen_desc">Для всех приложений и предложений на главном экране</string>

    <!-- Double tap to sleep toggle -->
    <string name="pref_dt_gesture_title">Перевести телефон в спящий режим</string>
    <string name="pref_dt_gesture_summary">При двойном нажатии на пустое место на главном экране</string>

    <!-- Icon pack -->
    <string name="icon_pack_title">Набор иконок</string>
    <string name="icon_pack_default_label">По умолчанию</string>
    <string name="icon_pack_add">Установить другие</string>
    <string name="icon_pack_no_market">Там нет доступного магазина приложений</string>

</resources>
